<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LumememmAppLaus.AbsolutePage"
             Title="Lumememm">
    <ContentPage.Content>
        <AbsoluteLayout BackgroundColor="Orange">
            <BoxView Color="White" AbsoluteLayout.LayoutBounds="0.5, 0.05, 200, 200"
                     CornerRadius="100" AbsoluteLayout.LayoutFlags="PositionProportional"/>
            <BoxView Color="White" AbsoluteLayout.LayoutBounds="0.5, 0.365, 225, 225"
                     CornerRadius="110" AbsoluteLayout.LayoutFlags="PositionProportional"/>
            <BoxView Color="White" AbsoluteLayout.LayoutBounds="0.5, 0.75, 250, 250"
                     CornerRadius="120" AbsoluteLayout.LayoutFlags="PositionProportional"/>
        </AbsoluteLayout>
    </ContentPage.Content>
</ContentPage>